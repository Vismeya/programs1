import java.io.*;
class find
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter a character");
        char c=sc.next();
        if((c>='a' && c<='z') || (c>='A' && c<='Z'))
        {
            System.out.println("c is a character");
        }
        else
        {
            System.out.println("c is not a character");
        }
    }
}
